<?xml version="1.0" encoding="UTF-8"?>  

<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               
	<bean name = "studentFromBP20" class = "com.example.sp.basic.Student" >
		<property name = "name">
			<value>Krishna</value>
		</property>
		<property name = "address">
			<value>Mirzapur</value>
		</property>
		<property name = "studentId">
			<value>11</value>
		</property>
	</bean>
	
	<bean name = "studentFromBP21" class = "com.example.sp.basic.Student" >
		<property name = "name" value = "Duggu" />
		<property name = "address" value = "MZP" />
		<property name = "studentId" value = "8" />
	</bean>
	
	<bean name = "studentFromBP22" class = "com.example.sp.basic.Student" p:name = "Keshav" p:address = "Gurgaon" p:studentId = "25" />
	
</beans>